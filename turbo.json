{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "NODE_ENV"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"],
      "env": [
        "DATABASE_URL",
        "SCRAPE_QUEUE_MODE",
        "PLAY_SCRAPE_MODE",
        "PLAY_SCRAPE_FALLBACK_PLAYWRIGHT"
      ]
    },
    "dev": {
      "cache": false,
      "env": ["*"]
    }
  }
}